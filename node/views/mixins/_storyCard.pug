mixin storyCard(story={"title":"Test"}, rule={})
    .story    
        .title 
            h2 #{story.title}
        span.page-divider
            span.one
            span.two
        - const storyBody = helpers.storyBody(story,rule) 
        .body !{storyBody}
        .author 
            | by 
            a(href=`/authors/${story.author}.htm`) #{story.author}
